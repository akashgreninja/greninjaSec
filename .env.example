# GreninjaSec Configuration
# Copy this file to .env and fill in your values

# OpenWebUI Configuration (Optional - for AI-enhanced attack chain analysis)
OPENWEBUI_URL=https://your-openwebui-instance.com/api
OPENWEBUI_TOKEN=sk-your-token-here

# AI Model to use (optional, defaults to gpt-3.5-turbo)
AI_MODEL=gpt-4

# Enable AI analysis by default (true/false)
AI_ENABLED=false

# Cache AI responses to avoid repeated API calls (true/false)
AI_CACHE_ENABLED=true
